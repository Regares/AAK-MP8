#include <stdio.h>
int main()
{
    int a, des, ed, new;
    printf("Введите двузначное целое число: ");
    scanf("%d", &a);
    if ((a < 10)  || (a > 99))
    {
        printf("Ошибка: Введите ДВУЗНАЧНОЕ ЧИСЛО.");
    }
    else
    {
        ed = a % 10;
        des = ((a - ed) % 100) / 10;
        new = des + ed * 10;
        printf("Новое число: %d", new);
    }
    return 0;
}
