#include <stdio.h>
int main()
{
    float a, b;
    int r, l;
    printf("Введите целые положительные числа A и B (A > B): ");
    scanf("%f%f", &a, &b);
    if ((a > (int)a) || (b > (int)b) || (b >= a))
    {
        printf("Числа не соответствуют условию.");
    }
    else
    {
        r = (int)a / b;
        l = (int)a - r * b;
        printf("Длина незанятой части отрезка A: %d", l);
    }
    return 0;
}
