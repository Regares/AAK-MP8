#include <stdio.h>
int main()
{
    int a, sot, des, ed, new;
    printf("Введите трехзначное целое число: ");
    scanf("%d", &a);
    if ((a < 100)  || (a > 999))
    {
        printf("Ошибка: Введите ТРЕХЗНАЧНОЕ ЧИСЛО.");
    }
    else
    {
        ed = (a % 10);
        des = ((a - ed) % 100);
        sot = ((a - ed - des) % 1000) / 100;
        new = des*10 + ed*10 + sot;
        printf("Новое число: %d", new);
    }
    return 0;
}
